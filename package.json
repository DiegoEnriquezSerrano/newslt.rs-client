{
  "name": "newslt.rs-client",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build-storybook": "storybook build",
    "build": "vite build",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "coverage": "vitest run --coverage",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test:e2e": "vitest run --project=client && playwright test --project=firefox",
    "test:unit": "vitest run --project=server",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "typecheck": "vitest typecheck"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.9",
    "@storybook/addon-docs": "^10.1.9",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/addon-vitest": "^10.1.9",
    "@storybook/sveltekit": "^10.1.9",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^22",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-storybook": "^10.1.9",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^16.5.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.0",
    "storybook": "^10.1.9",
    "svelte": "^5.45.6",
    "svelte-adapter-bun": "^1.0.1",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vitest": "^4.0.15",
    "vitest-browser-svelte": "^2.0.1"
  }
}
